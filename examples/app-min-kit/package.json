{
  "name": "app-min-kit",
  "version": "0.5.10",
  "private": true,
  "description": "min-kit examples of taro miniapp project",
  "author": "rexerwang",
  "license": "MIT",
  "repository": "rexerwang/min-kit",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "Sass",
    "framework": "React"
  },
  "scripts": {
    "build:weapp": "taro prebuild --type weapp && taro build --type weapp",
    "build:swan": "taro prebuild --type swan && taro build --type swan",
    "build:alipay": "taro prebuild --type alipay && taro build --type alipay",
    "build:tt": "taro prebuild --type tt && taro build --type tt",
    "build:h5": "taro prebuild --type h5 && taro build --type h5",
    "build:rn": "taro prebuild --type rn && taro build --type rn",
    "build:qq": "taro prebuild --type qq && taro build --type qq",
    "build:jd": "taro prebuild --type jd && taro build --type jd",
    "build:quickapp": "taro prebuild --type quickapp && taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "clean": "rimraf node_modules/.cache node_modules/.taro .swc .temp dist coverage"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "dependencies": {
    "@babel/runtime": "^7.23.8",
    "@min-kit/components": "workspace:*",
    "@min-kit/extends": "workspace:*",
    "@min-kit/helper": "workspace:*",
    "@min-kit/hooks": "workspace:*",
    "@min-kit/shared": "workspace:*",
    "@min-kit/store": "workspace:*",
    "@tarojs/components": "3.6.23",
    "@tarojs/helper": "3.6.23",
    "@tarojs/plugin-framework-react": "3.6.23",
    "@tarojs/plugin-platform-alipay": "3.6.23",
    "@tarojs/plugin-platform-jd": "3.6.21",
    "@tarojs/plugin-platform-qq": "3.6.23",
    "@tarojs/plugin-platform-swan": "3.6.23",
    "@tarojs/plugin-platform-tt": "3.6.23",
    "@tarojs/plugin-platform-weapp": "3.6.23",
    "@tarojs/react": "3.6.23",
    "@tarojs/runtime": "3.6.23",
    "@tarojs/shared": "3.6.23",
    "@tarojs/taro": "3.6.23",
    "clsx": "^2.1.0",
    "highlight.js": "^11.9.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.5",
    "@tarojs/cli": "3.6.23",
    "@tarojs/plugin-mini-ci": "3.6.23",
    "@tarojs/taro-loader": "3.6.23",
    "@tarojs/webpack5-runner": "3.6.23",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.0",
    "@types/webpack-env": "^1.13.6",
    "babel-loader": "^9.1.3",
    "babel-preset-taro": "3.6.23",
    "postcss": "^8.4.33",
    "react-refresh": "^0.11.0",
    "tailwindcss": "^3.4.1",
    "tsconfig-paths-webpack-plugin": "^4.0.1",
    "typescript": "^5.1.0",
    "webpack": "5.90.2"
  }
}
