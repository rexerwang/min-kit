{
  "name": "@miniapp/taro-config",
  "version": "0.1.0",
  "description": "enhance taro config",
  "author": "rexerwang",
  "license": "MIT",
  "repository": "rexerwang/taro-miniapp",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
