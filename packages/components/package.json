{
  "name": "@miniapp/components",
  "version": "0.1.0",
  "description": "miniapp React components",
  "author": "rexerwang",
  "license": "MIT",
  "repository": "rexerwang/taro-miniapp",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./dist/styles/*.css": {
      "import": "./dist/styles/*.css",
      "require": "./dist/styles/*.css"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@miniapp/extends": "workspace:*",
    "@miniapp/hooks": "workspace:*",
    "@miniapp/shared": "workspace:*",
    "@miniapp/store": "workspace:*",
    "clsx": "^2.0.0"
  },
  "peerDependencies": {
    "@tarojs/components": ">=3.6",
    "@tarojs/plugin-platform-alipay": ">=3.6",
    "@tarojs/plugin-platform-h5": ">=3.6",
    "@tarojs/plugin-platform-jd": ">=3.6",
    "@tarojs/plugin-platform-qq": ">=3.6",
    "@tarojs/plugin-platform-swan": ">=3.6",
    "@tarojs/plugin-platform-tt": ">=3.6",
    "@tarojs/plugin-platform-weapp": ">=3.6",
    "@tarojs/react": ">=3.6",
    "@tarojs/taro": ">=3.6",
    "react": ">=17",
    "react-dom": ">=17"
  },
  "devDependencies": {
    "@miniapp/helper": "workspace:*"
  }
}
