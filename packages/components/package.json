{
  "name": "@min-kit/components",
  "version": "0.5.10",
  "description": "React components for miniapp",
  "author": "rexerwang",
  "license": "MIT",
  "repository": "rexerwang/min-kit",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./dist/styles/*.css": {
      "import": "./dist/styles/*.css",
      "require": "./dist/styles/*.css"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "styles"
  ],
  "sideEffects": [
    "*.css",
    ".scss"
  ],
  "keywords": [
    "min-kit",
    "components",
    "react",
    "taro",
    "miniapp",
    "miniprogram"
  ],
  "scripts": {
    "build": "unbuild",
    "test": "jest"
  },
  "dependencies": {
    "@min-kit/extends": "workspace:*",
    "@min-kit/hooks": "workspace:*",
    "@min-kit/shared": "workspace:*",
    "@min-kit/store": "workspace:*",
    "clsx": "^2.1.0"
  },
  "peerDependencies": {
    "@tarojs/components": ">=3.6",
    "@tarojs/plugin-platform-alipay": ">=3.6",
    "@tarojs/plugin-platform-h5": ">=3.6",
    "@tarojs/plugin-platform-jd": ">=3.6",
    "@tarojs/plugin-platform-qq": ">=3.6",
    "@tarojs/plugin-platform-swan": ">=3.6",
    "@tarojs/plugin-platform-tt": ">=3.6",
    "@tarojs/plugin-platform-weapp": ">=3.6",
    "@tarojs/react": ">=3.6",
    "@tarojs/taro": ">=3.6",
    "react": ">=17",
    "react-dom": ">=17"
  },
  "peerDependenciesMeta": {
    "@tarojs/plugin-platform-alipay": {
      "optional": true
    },
    "@tarojs/plugin-platform-h5": {
      "optional": true
    },
    "@tarojs/plugin-platform-jd": {
      "optional": true
    },
    "@tarojs/plugin-platform-qq": {
      "optional": true
    },
    "@tarojs/plugin-platform-swan": {
      "optional": true
    },
    "@tarojs/plugin-platform-tt": {
      "optional": true
    },
    "@tarojs/plugin-platform-weapp": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "devDependencies": {
    "@min-kit/helper": "workspace:*",
    "@min-kit/jest": "workspace:^",
    "cssnano": "^6.0.3",
    "postcss-cli": "^11.0.0",
    "tailwindcss": "^3.4.1"
  }
}
