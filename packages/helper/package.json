{
  "name": "@min-kit/helper",
  "version": "0.5.4",
  "description": "Plugins & configs of Tarojs for miniapp",
  "author": "rexerwang",
  "license": "MIT",
  "repository": "rexerwang/min-kit",
  "exports": {
    ".": {
      "types": "./dist/taro-env.d.ts"
    },
    "./compile": {
      "types": "./dist/compile/index.d.ts",
      "import": "./dist/compile/index.mjs",
      "require": "./dist/compile/index.cjs"
    },
    "./compile/plugin-prebuild": {
      "types": "./dist/compile/plugin-prebuild.d.ts",
      "import": "./dist/compile/plugin-prebuild.mjs",
      "require": "./dist/compile/plugin-prebuild.cjs"
    },
    "./runtime": {
      "types": "./dist/runtime/index.d.ts",
      "import": "./dist/runtime/index.mjs",
      "require": "./dist/runtime/index.cjs"
    },
    "./config": {
      "types": "./dist/config/index.d.ts",
      "import": "./dist/config/index.mjs",
      "require": "./dist/config/index.cjs"
    }
  },
  "types": "./dist/taro-env.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "min-kit",
    "helper",
    "config",
    "taro",
    "miniapp",
    "miniprogram"
  ],
  "scripts": {
    "build": "unbuild",
    "test": "jest"
  },
  "peerDependencies": {
    "@tarojs/cli": ">=3.6",
    "@tarojs/helper": ">=3.6",
    "@tarojs/plugin-mini-ci": ">=3.6",
    "@tarojs/service": ">=3.6",
    "@tarojs/taro": ">=3.6"
  },
  "dependencies": {
    "autoprefixer": "^10.4.16",
    "image-minimizer-webpack-plugin": "^3.8.3",
    "imagemin": "^8.0.1",
    "minimist": "^1.2.8",
    "postcss": "^8.4.32",
    "postcss-rem-to-responsive-pixel": "^6.0.1",
    "sharp": "^0.33.1",
    "svgo": "^3.2.0",
    "tailwindcss": "^3.4.1",
    "weapp-tailwindcss": "^3.0.4",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-merge": "^5.10.0"
  },
  "devDependencies": {
    "@min-kit/jest": "workspace:*"
  }
}
